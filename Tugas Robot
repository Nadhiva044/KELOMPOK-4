hurdle 1
def turn_right():
    turn_left()
    turn_left()
    turn_left()
    
def lewat_gawang():
    move()
    turn_left()
    move()
    turn_right()
    move()
    turn_right()
    move()
    turn_left()
    
while (not at_goal()):
    lewat_gawang()

hurdle 2
def turn_right():
  turn_left()
  turn_left()
  turn_left()

def lewat_gawang():
  move()
  turn_left()
  move()
  turn_right()
  move()
  turn_right()
  move()
  turn_left()

while(not at_goal()):
  lewat_gawang()
